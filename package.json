{
  "name": "dialect-ui",
  "private": false,
  "version": "0.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/dialect-ui.umd.cjs",
  "module": "./dist/dialect-ui.js",
  "exports": {
    ".": {
      "import": "./dist/dialect-ui.js",
      "require": "./dist/dialect-ui.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "rm -rf dist",
    "build": "tsc -b && vite build",
